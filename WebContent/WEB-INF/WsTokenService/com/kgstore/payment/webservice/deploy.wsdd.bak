<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from WsTokenService WSDL service -->

  <service name="WsToken" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservice.payment.kgstore.com"/>
      <parameter name="wsdlServiceElement" value="WsTokenService"/>
      <parameter name="schemaQualified" value="http://webservice.payment.kgstore.com"/>
      <parameter name="wsdlServicePort" value="WsToken"/>
      <parameter name="className" value="com.kgstore.payment.webservice.WsTokenSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="WsToken"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getToken" qname="operNS:getToken" xmlns:operNS="http://webservice.payment.kgstore.com" returnQName="retNS:getTokenReturn" xmlns:retNS="http://webservice.payment.kgstore.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://webservice.payment.kgstore.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getToken"/>

  </service>
</deployment>
